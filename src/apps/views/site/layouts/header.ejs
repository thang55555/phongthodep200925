                                        <% for(item of menutt){%>
                                            <li>
                                                <a href="/category-tintuc/<%=item.slug%>/<%=item._id%>"
                                                    title="<%=item.menutintuc%>" class="parent__ pl-0" style="text-transform: uppercase;">
                                                    <%=item.menutintuc%>
                                                </a>
                                                <i></i>
