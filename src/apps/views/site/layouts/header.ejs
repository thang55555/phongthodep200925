<base href="/static/site/">
<!-- Css Styles -->
<link rel="shortcut icon" type="image/ico" href="images/update/logo.ico"/>

<link rel="stylesheet" href="css/style.css" type="text/css">

</head>

<body >




<div class="body_img">
