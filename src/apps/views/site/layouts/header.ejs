                                <li class="clazzli clazzli-38 bind-ajax"><a class="clazza clazza-38" rel="">Dịch Vụ</a>
                                    <ul>
                                        <% for(item of menudv){%>
                                            <li>
                                                <a href="/category-dich-vu/<%=item.slug%>/<%=item._id%>"
                                                    title="<%=item.menudichvu%>" class="parent__ pl-0">
                                                    <%=item.menudichvu%>
                                                </a>
                                                <i></i>
                                            </li>
                                            <%}%>
                                    </ul>
                                </li>

                                <li class="clazzli clazzli-12 bind-ajax "><a class="clazza clazza-12" rel="">Sản
                                        Phẩm</a>
