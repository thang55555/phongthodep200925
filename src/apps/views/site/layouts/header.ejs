    <div>
        <header class="header">
            <h1></h1>
            <div class="tops-headers">
                <div class="container">
                    <ul class="list-address__headers">
                        <%for(item of thongtintrang){%>
                            <li><a href="tel:<%=item.sdt%>" title="<%=item.sdt%>"><i class="fa fa-phone"
                                        aria-hidden="true"></i>
                                    <%=item.sdt%>
                                </a></li>
                            <li><a href="mailto:<%=item.email%>" title="<%=item.email%>"><i class="fa fa-envelope"
                                        aria-hidden="true"></i>
                                    <%=item.email%>
                                </a>
                            </li>
                            <%}%>
                    </ul> <a href="/tu-van" title="Gửi yêu cầu tư vấn" class="btn-address__headers">Gửi yêu cầu tư
                        vấn</a>
                </div>
            </div>
            <div class="logo-header"> <a href="/">
                    <% for(item of thongtintrang){%>
                        <picture>
                            <img src="../site/images/update/<%=item.images%>" class="img-fluid lazyload" value="logo" alt="<%=item.images%>">
                        </picture>
                        <%}%>
                </a>
            </div>
            <div class="content-headers">
                <div class="container"> <span class="button-phone btn_sp_menu"><i class="fa fa-bars"
