<footer style="margin-top: 30px;">
    <section class="top-footers">
        <div class="container">
            <ul>
                <% for(item of MENU){%>
                <li class="clazzli clazzli-5 "><a class="clazza clazza-5" rel="" href="/category-menu/<%=item.slug%>/<%=item._id%>"><%=item.tendanhmuc%></a></li>
                <%}%>
            </ul>
        </div>
    </section>
    <section class="content-footers">
        <%for(item of thongtintrang){%>
        <div class="container">
            <div class="row">
                <div class="col-lg-4">
                    <div class="box-contents__footer"> <a href="/" title="Nam Thành Phát" class="logo-footers">
                            <picture>
                                <img src="./images/<%=item.images%>" data-src="#" title="" alt="">
