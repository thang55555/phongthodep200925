                        <p class="titles-footers"><%=item.tencongty%></p>
                        <ul class="list-adress__footers">
                            <li><i class="fa fa-map-marker" aria-hidden="true"></i><%=item.diachicty%></li>
                            <li><i class="fa fa-envelope" aria-hidden="true"></i><a
                                    href="mailto:<%=item.email%>"
                                    title="<%=item.email%>"><%=item.email%></a></li>
                            <li><i class="fa fa-phone" aria-hidden="true"></i><a href="tel:<%=item.sdt%>"
                                    title="<%=item.sdt%>"><%=item.sdt%></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-12">
                    <div class="intros-adress__footers">
                        <p class="titles-footers"><%=item.tieudeFB%></p>
                        <ul class="list-adress__footers">
                            <li><i class="fa fa-map-marker" aria-hidden="true"></i><%=item.diachiFB%></li>
                            <li><i class="fa fa-envelope" aria-hidden="true"></i><a
                                    href="mailto:<%=item.email%>"
                                    title="<%=item.email%>"><%=item.email%></a></li>
                            <li><i class="fa fa-phone" aria-hidden="true"></i><a href="tel:<%=item.sdt%>"
                                    title="<%=item.sdt%>"><%=item.sdt%></a></li>
                        </ul>
                    </div>
                </div>
