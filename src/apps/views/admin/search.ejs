<%- include("./layout/head", {title: "Product" })%>
    <%- include("./layout/header")%>
        <%- include("./layout/siderbar")%>


            <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
                <div class="row">
                    <ol class="breadcrumb">
                        <li><a href="#"><svg class="glyph stroked home">
                                    <use xlink:href="#stroked-home"></use>
                                </svg></a></li>
                        <li>Search</li>
                    </ol>
                </div><!--/.row-->

                <div class="row">
                    <div class="col-lg-12">
                        <h2>Có <%=total%> kết quả tìm kiếm: <%=keyword%></h2>
                    </div>
                    <div class="col-lg-12" style="margin-left: 15px;">
                        <%for(item of products){%>
                        <div style="margin-bottom: 10px; border-bottom: 1px #ccc solid;">

                            <h3 class="LC20lb MBeuO DKV0Md"><a href="/admin/edit-san-pham/<%=item._id%>"><%=item.tieudesp%> </a></h3>
                            <div><%= new Intl.DateTimeFormat('en-US').format(item.createdAt)%> — <%=item.metadescription%></div>

                        </div>
                        <%}%>


                        <%for(item of product){%>
                            <div style="margin-bottom: 10px; border-bottom: 1px #ccc solid;">
    
                                <h3 class="LC20lb MBeuO DKV0Md"><a href="/admin/edit-bai-viet-dich-vu/<%=item._id%>"><%=item.tieudesp%> </a></h3>
                                <div><%= new Intl.DateTimeFormat('en-US').format(item.createdAt)%> — <%=item.metadescription%></div>
    
                            </div>
                            <%}%>


                            <%for(item of product2){%>
                                <div style="margin-bottom: 10px; border-bottom: 1px #ccc solid;">
        
                                    <h3 class="LC20lb MBeuO DKV0Md"><a href="/admin/edit-bai-viet-tin-tuc/<%=item._id%>"><%=item.tieudebaiviet%> </a></h3>
                                    <div><%= new Intl.DateTimeFormat('en-US').format(item.createdAt)%> — <%=item.metadescription%></div>
        
                                </div>
                                <%}%>
                    </div>
                </div><!--/.row-->


            </div> <!--/.main-->

            <script src="js/jquery-1.11.1.min.js"></script>
            <script src="js/bootstrap.min.js"></script>
            <script src="js/bootstrap-table.js"></script>

            <%- include("./layout/footer")%>